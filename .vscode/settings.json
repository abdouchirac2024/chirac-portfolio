{
  "css.validate": false,
  "less.validate": false,
  "scss.validate": false,
  "tailwindCSS.includeLanguages": {
    "typescript": "javascript",
    "typescriptreact": "javascript"
  },
  "tailwindCSS.experimental.classRegex": [
    "class[Name]*\\s*[:=]\\s*[\"'`]([^\"'`]*)[\"'`]",
    "className\\s*[:=]\\s*[\"'`]([^\"'`]*)[\"'`]"
  ],
  "files.associations": {
    "*.css": "tailwindcss"
  }
}